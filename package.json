{
  "name": "conclave",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node-dev ./bin/dev",
    "build": "babel src --out-dir dist",
    "pretest": "export NODE_ENV=test && node src/utils/db-setup.js",
    "test": "multi=\"mocha-osx-reporter=- mocha-better-spec-reporter=-\" mocha \"src/**/*-spec.js\" --reporter mocha-multi --require=env-test --compilers js:babel-register --recursive",
    "posttest": "export NODE_ENV=development",
    "model:create": "node bin/model.js ",
    "migrate:create": "node bin/migration.js ",
    "migrate:run": "knex migrate:latest",
    "seed:run": "knex seed:run",
    "seed:make": "knex seed:make",
    "fixtures": "node src/utils/fixtures.js",
    "precov": "export NODE_ENV=test",
    "cov": "nyc npm test",
    "fe:build": "webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "plato": "es6-plato -r -d report src",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "bulma": "^0.2.1",
    "chalk": "^2.4.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.2",
    "es6-plato": "^1.0.18",
    "exlist": "^0.7.0",
    "express": "^4.14.0",
    "express-conditional-middleware": "^1.1.2",
    "express-ejs-layouts": "^2.2.0",
    "express-graphql": "^0.5.4",
    "express-list-endpoints": "^4.0.0",
    "express-session": "^1.14.1",
    "flash": "^1.1.0",
    "graphql": "^0.7.2",
    "graphql-bookshelf": "0.0.8",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.5",
    "mysql": "^2.11.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "requiredir": "^1.0.7",
    "serve-favicon": "^2.3.0",
    "to-case": "^2.0.0",
    "xtconf": "^2.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kev_nz/eslint-config": "^1.0.0",
    "approvals": "^3.0.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "env-test": "^1.0.0",
    "eslint": "^5.7.0",
    "faker": "^3.1.0",
    "getstorybook": "^1.6.1",
    "html-webpack-plugin": "^2.24.1",
    "js-object-pretty-print": "^0.2.0",
    "lorem-ipsum": "^1.0.3",
    "mocha": "^3.1.2",
    "mocha-better-spec-reporter": "^3.0.2",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "node-dev": "^3.1.3",
    "nyc": "^8.3.1",
    "pluralize": "^3.0.0",
    "supertest": "^2.0.0",
    "webpack": "^1.13.3",
    "yargs": "^6.0.0"
  }
}
